<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : chatbotmessages.xml
    Author     : Carlos Garcia Gomez
    Description:
        Structure for the chatbot_messages table.
-->
<table>
    <column>
        <name>content</name>
        <type>text</type>
    </column>
    <column>
        <name>creationtime</name>
        <type>integer</type>
        <null>NO</null>
    </column>
    <column>
        <name>humanid</name>
        <type>character varying(50)</type>
        <null>NO</null>
    </column>
    <column>
        <name>idchat</name>
        <type>serial</type>
        <null>NO</null>
        <default>nextval('chatbot_messages_idchat_seq'::regclass)</default>
    </column>
    <column>
        <name>ischatbot</name>
        <type>boolean</type>
        <null>NO</null>
        <default>false</default>
    </column>
    <column>
        <name>unmatched</name>
        <type>boolean</type>
        <null>NO</null>
        <default>false</default>
    </column>
    <constraint>
        <name>chatbot_messages_pkey</name>
        <type>PRIMARY KEY (idchat)</type>
    </constraint>
</table>