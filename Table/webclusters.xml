<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : webpages.xml
    Author     : Carlos Garcia Gomez
    Description:
        Structure for the webcluster table.
-->
<table>
    <column>
        <name>description</name>
        <type>character varying(500)</type>
        <null>NO</null>
    </column>
    <column>
        <name>idcluster</name>
        <type>serial</type>
        <null>NO</null>
        <default>nextval('webclusters_idcluster_seq'::regclass)</default>
    </column>
    <column>
        <name>title</name>
        <type>character varying(200)</type>
        <null>NO</null>
    </column>
    <constraint>
        <name>webclusters_pkey</name>
        <type>PRIMARY KEY (idcluster)</type>
    </constraint>
</table>